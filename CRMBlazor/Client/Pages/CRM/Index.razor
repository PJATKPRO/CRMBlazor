@page "/CRM"
@page "/CRM/Index/{Counter:int?}"


<h3>Index</h3>


<h1>Title @Counter</h1>

<strong class="alert-danger">@list.Count</strong>

@foreach(var item in list)
{
    <h4>@item</h4>
}



@code {

    [Parameter]
    public int Counter { get; set; }

    private List<string> list;

    protected override async Task OnParametersSetAsync()
    {

        list = new() { "ab", "cd", "ef" };

        

    }



}
